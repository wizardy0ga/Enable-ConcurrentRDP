Write-Host "[*] Got new configuration. Stopping TermService and replacing configuration.";Stop-Service TermService -Force;Set-Content -Path ("{0}\rdpwrap.ini" -f $PROGRAM_FOLDER) -Value $Request.Content -Force;Start-Service TermService;if ((Get-Service -Name TermService).Status -eq "Running") {Write-Host "[+] Started TermService. Multi session RDP should now be available."}else{Write-Host "[-] TermService is not running. An issue has occurred."}Invoke-CleanUp